{
  "name": "documents-app",
  "version": "1.0.0",
  "scripts": {
    "dev": "webpack-dev-server --inline --hot --content-base --mode development --config webpack.dev.config",
    "generate-sass-typings": "webpack --mode production --config webpack.sass.typings.config",
    "prod": "webpack --mode production --config webpack.config",
    "watch": "webpack --mode development --config webpack.dev.config --watch",
    "tslint": "tslint \"app/**/*.{ts,tsx}\" -c tslintCli.json",
    "prettier": "prettier --write \"app/**/*.tsx\" \"tests/**/*.tsx\"",
    "prettiercss": "prettier --parser css --write \"app/themes/*.scss\"",
    "precommit": "npm run prettier && npm run prettiercss && npm run tslint && npm run prod"
  },
  "author": "Muhammed",
  "license": "ISC",
  "devDependencies": {
    "@types/react-router-dom": "^4.3.1",
    "awesome-typescript-loader": "^5.2.1",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.3",
    "css-loader": "^1.0.0",
    "html-webpack-externals-plugin": "^3.8.0",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.9.4",
    "prettier": "^1.14.3",
    "raw-loader": "^0.5.1",
    "require-context": "^1.1.0",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "tslint": "^5.11.0",
    "tslint-loader": "^3.5.4",
    "tslint-react": "^3.6.0",
    "typescript": "^3.1.3",
    "typings-for-css-modules-loader": "^1.7.0",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "@types/react": "^16.4.17",
    "@types/react-dom": "^16.0.9",
    "axios": "^0.18.1",
    "moment": "^2.22.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "rxjs": "^6.3.3"
  },
  "prettier": {
    "singleQuote": true,
    "tabWidth": 2,
    "useTabs": false,
    "parser": "typescript",
    "printWidth": 120,
    "bracketSpacing": true,
    "arrowParens": "always"
  }
}
